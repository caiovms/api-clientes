{
  "ReRoutes": [
    {
      "UpstreamPathTemplate": "/contatos",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Contato.ContatoService/Listar",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_clientes",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/contatos/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Contato.ContatoService/Obter",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_clientes",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/contatos",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamPathTemplate": "/Contato.ContatoService/Inserir",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_clientes",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/contatos",
      "UpstreamHttpMethod": [ "PUT" ],
      "DownstreamPathTemplate": "/Contato.ContatoService/Alterar",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_clientes",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/contatos",
      "UpstreamHttpMethod": [ "DELETE" ],
      "DownstreamPathTemplate": "/Contato.ContatoService/Excluir",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_clientes",
          "Port": 80
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Hey, you request too many times, slow down!!!",
      "HttpStatusCode": 999
    }
  }
}