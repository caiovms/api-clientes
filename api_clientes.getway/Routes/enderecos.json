{
  "ReRoutes": [
    {
      "UpstreamPathTemplate": "/enderecos",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Endereco.EnderecoService/Listar",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_enderecos",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/enderecos/{id}",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Endereco.EnderecoService/Obter",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_enderecos",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/enderecos",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamPathTemplate": "/Endereco.EnderecoService/Inserir",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_enderecos",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/enderecos",
      "UpstreamHttpMethod": [ "PUT" ],
      "DownstreamPathTemplate": "/Endereco.EnderecoService/Alterar",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_enderecos",
          "Port": 80
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/enderecos",
      "UpstreamHttpMethod": [ "DELETE" ],
      "DownstreamPathTemplate": "/Endereco.EnderecoService/Excluir",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "service_grpc_enderecos",
          "Port": 80
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "RateLimitOptions": {
      "DisableRateLimitHeaders": false,
      "QuotaExceededMessage": "Hey, you request too many times, slow down!!!",
      "HttpStatusCode": 999
    }
  }
}